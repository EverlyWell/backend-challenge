le-module--layoutExampleData--pf4d8{border:2px solid rgba(0,0,200,.7)}.style-module--layoutExampleInteraction--2Hz8x{border:2px solid rgba(200,0,0,.7)}.style-module--layoutExplodedData--6uH5j{transform:translateX(-40px) translateZ(-100px) rotateY(36deg)}.style-module--layoutExplodedInteraction--2_dwR{transform:translateX(-40px) translateZ(-100px) rotateY(36deg) translateZ(150px)}.style-module--teaser--1nXS9{font-size:15px;max-width:790px}.style-module--teaser--1nXS9 a{background-color:#f0f0f0;padding:15px;min-height:120px;display:block;text-decoration:none;color:inherit;display:flex;flex-direction:column;align-items:flex-start;border-radius:5px;color:inherit!important}@media (min-width:481px){.style-module--teaser--1nXS9 a{background-image:url(/static/explain_schema_reference-8e0707e6c2a8593c18d628bba58f62bc.png);background-size:contain;background-position:115% bottom;background-repeat:no-repeat;padding-right:280px;border-right:1px solid #f0f0f0}}.style-module--teaser--1nXS9 a h3{line-height:1em!important;font-size:18px;margin-bottom:5px}.style-module--teaser--1nXS9 a p{margin:10px 20px 12px 0!important;max-width:500px!important}.style-module--teaser--1nXS9 a .style-module--teaserButton--3TmV6{background-color:#4ba64b;color:#fff;padding:7px 25px;border-radius:3px}.style-module--sql--3eJkq{display:block;font-family:monospace;white-space:pre-wrap;-webkit-hyphens:none;-ms-hyphens:none;hyphens:none}.style-module--keyword--3eNF4{text-transform:uppercase;color:#090;word-break:normal}.style-module--aggregate--2JY_7{color:inherit}.style-module--number--2QX9c,.style-module--string--GZgC9{color:#555}.style-module--comment--2BO-s{color:#888}.style-module--noborder--1ze8y{border:none;margin:0;padding:0;background:none}.style-module--inline--2JSnO{display:inline}</style><meta name="generator" content="Gatsby 2.29.3"/><title data-react-helmet="true">Full Text Search in Milliseconds with Rails and PostgreSQL</title><meta data-react-helmet="true" name="viewport" content="width=devi